import numpy as np
import copy

arr = np.array([[0, 1, 2, 3, 4, 5],
                [10, 11, 12, 13, 14, 15],
                [20, 21, 22, 23, 24, 25],
                [30, 31, 32, 33, 34, 35],
                [40, 41, 42, 43, 44, 45],
                [50, 51, 52, 53, 54, 55]])

arr_data = copy.deepcopy(arr)
pink = arr_data[1,2:4]
green = arr_data[2:4,4::]
blue = arr_data[0::,1]
orange = arr_data[2:5:2,0:5:2]

print(pink)
print(green)
print(blue)
print(orange)